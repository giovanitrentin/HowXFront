,"sourcesContent":["<template>\n  <v-row justify=\"center\">\n    <v-col\n      cols=\"12\"\n      sm=\"10\"\n      md=\"8\"\n      lg=\"6\"\n    >\n      <v-divider class=\"mt-12\"></v-divider>\n      <h2 center>Cadastro de Objetos do Imóvel</h2>\n\n      <v-card ref=\"form\">\n        <v-divider class=\"mt-12\"></v-divider>\n          <v-card-text>\n            <v-select  \n              v-model=\"objetos.idImovel\"\n              :items=\"imoveis\"\n              single-line\n              label=\"Selecione o imóvel\"\n            ></v-select>\n      \n            <v-text-field\n              ref=\"nome\"\n              v-model=\"objetos.nome\"\n              label=\"Nome do objeto\"\n              required\n            ></v-text-field>\n          \n            <v-text-field\n              ref=\"quantidade\"\n              v-model=\"objetos.quantidade\"\n              label=\"Quantidade\"\n              required\n            ></v-text-field>\n\n            <v-textarea\n              ref=\"descricao\"\n              v-model=\"objetos.descricao\"\n              label=\"Descrição\"\n              required\n            ></v-textarea>\n          </v-card-text>\n        <v-divider class=\"mt-12\"></v-divider>\n        \n        <v-card-actions>\n          <v-btn \n          text\n          @click=\"volta\">\n            Cancel\n          </v-btn>\n          <v-spacer></v-spacer>\n          <v-slide-x-reverse-transition>\n            <v-tooltip\n              left\n                          \n            >\n            </v-tooltip>\n          </v-slide-x-reverse-transition>\n          <v-btn\n            color=\"primary\"\n            text\n            @click=\"submit\"\n          >\n            Submit\n          </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-col>\n  </v-row>\n</template>\n\n<script>\n\n  import Objeto from  '@/services/objetos'\n  import Imovel from  '@/services/imoveis'\n\n  export default {\n\n    data () {          \n      return {\n        objetos: {},\n        imoveis: []\n      }     \n    },\n\n    mounted(){\n      var idObjeto = this.$route.params.id;\n      if (idObjeto == 0){\n          console.log(idObjeto)\n          this.objetos = {}\n      }        \n      else{\n        console.log(idObjeto)\n        Objeto.get(idObjeto).then(resposta => {\n          console.log(resposta.data)\n          this.objetos = resposta.data\n        })\n      }\n      Imovel.getAll().then(resposta => {\n        console.log(resposta.data)\n        this.imoveis = resposta.data\n      })\n    },\n\n    methods:{\n      submit(){\n        if (this.$route.params.id == 0) {\n          Objeto.create(this.objetos)\n          this.$router.push({name: 'usuariomanutencao'}); \n        }        \n        else {\n          Objeto.update(this.$route.params.id, this.objetos)\n          this.$router.push({name: 'usuariomanutencao'}); \n        } \n      },\n      volta(){\n        this.$router.push({name: 'usuariomanutencao'}); \n      }\n    }\n\n  }\n</script>"]},"metadata":{},"sourceType":"module"}